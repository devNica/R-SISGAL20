<div class="container">
    <div class="border py-4 px-4 my-5">
        <table class="table table-striped table-bordered table-sm">
            <thead>
                <tr>
                <th scope="col" class="table-column-title">#</th>
                <th scope="col" class="table-column-title">WAREHOUSE NAME</th>
                <th scope="col" class="table-column-title">STATE</th>
                <th scope="col" class="table-column-title">OPTIONS</th>
                </tr>
            </thead>
            <tbody>
                {{#each warehouses}}
                <tr>
                    <th scope="row" class="table-row-content">{{this.id}}</th>
                    <td class="table-row-content">{{this.warehouse}}</td>
                    <td class="table-row-content">{{this.state}}</td>
                    <td class="table-row-content">
                        <a href="/sisgal/api/v1.0/warehouse-create" class="btn btn-sm btn-primary"><i class="fas fa-plus-square"></i></a>
                        <a href="/sisgal/api/v1.0/warehouse-edit/{{this.id}}" class="btn btn-sm btn-warning"><i class="fas fa-edit"></i></a>
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>


    </div>
</div>